[kdcdefaults]
    kdc_tcp_ports = 88, 750
    v4_mode = nopreauth

[realms]
    {{ realm }} = {
        databse_name = /var/kerberos/krb5kdc/principal
        master_key_type = aes256-cts
        acl_file = /var/kerberos/krb5kdc/kadm5.acl
        dict_file = /usr/share/dict/words
        key_stash_file = /var/kerberos/krb5kdc/.{{ realm }}
#        admind_port = 789
        default_principal_flags = +preauth, +renewable
        max_life = 24h 0m 0s
        max_renewable_life = 7d 0h 0m 0s
        admin_keytab = /var/kerberos/krb5kdc/kadm5.keytab
        supported_enctypes = aes256-cts:normal aes128-cts:normal des3-hmac-sha1:normal arcfour-hmac:normal des-cbc-md5:normal
        kdc_supported_enctypes = aes256-cts:normal aes128-cts:normal des3-hmac-sha1:normal arcfour-hmac:normal des-cbc-md5:normal
     }